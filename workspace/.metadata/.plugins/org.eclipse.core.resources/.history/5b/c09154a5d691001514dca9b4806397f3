package edu.iut.app;

import java.util.ArrayList;

import javax.swing.JOptionPane;

public class Personne extends ArrayList<ExamEvent> implements IExamEvent {

	@Override
	public ArrayList<ExamEvent> critere(ArrayList<ExamEvent> examEvents) {
		ArrayList <ExamEvent> persExam = new ArrayList<ExamEvent>();
		
		
		for (ExamEvent ee: examEvents){
			if(ee.getStudent() != null)
			persExam.add(ee);	
			else {
				JOptionPane.showMessageDialog(null, "Personne non repertorié !", "Nom inconnu", JOptionPane.ERROR_MESSAGE, null);
			}
		}
		
		return persExam;
	}	
}
	
