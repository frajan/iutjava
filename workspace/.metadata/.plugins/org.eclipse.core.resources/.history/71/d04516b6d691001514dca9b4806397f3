package edu.iut.app;

import java.util.ArrayList;

import javax.swing.JOptionPane;

public class Salle extends ArrayList<ExamEvent> implements IExamEvent {
	@Override
	public ArrayList<ExamEvent> critere(ArrayList<ExamEvent> examEvents) {
		ArrayList <ExamEvent> salleExam = new ArrayList<ExamEvent>();
		
		
		for (ExamEvent ee: examEvents){
			if(ee.getClassroom() != null)
			salleExam.add(ee);	
			else {
				JOptionPane.showMessageDialog(null, "", " inconnu", JOptionPane.ERROR_MESSAGE, null);
			}
		}
		
		return salleExam;
}

}