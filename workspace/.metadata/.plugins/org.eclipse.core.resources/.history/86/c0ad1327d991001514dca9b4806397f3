package edu.iut.app;

import java.util.ArrayList;

import javax.swing.JOptionPane;

public class Salle extends ArrayList<ExamEvent> implements IExamEvent {
	@Override
	public ArrayList<ExamEvent> critere(ArrayList<ExamEvent> examEvents,Object o) {
		ArrayList <ExamEvent> salleExam = new ArrayList<ExamEvent>();
		if( o instanceof Salle){
			for (ExamEvent ee: examEvents){
				if(ee.getClassroom() != null)
				salleExam.add(ee);	
			}
			
		}
		return salleExam;
		
		
		
		
	}

	@Override
	public ArrayList<ExamEvent> critere(ArrayList<ExamEvent> examEvent, Person p) {
		// TODO Auto-generated method stub
		return null;
	}

}