package edu.iut.app;

import java.util.ArrayList;

import javax.swing.JOptionPane;

public class Salle extends ArrayList<ExamEvent> implements IExamEvent {
	@Override
	public ArrayList<ExamEvent> critere(ArrayList<ExamEvent> examEvents,Salle s) {
		ArrayList <ExamEvent> salleExam = new ArrayList<ExamEvent>();
		
		
		for (ExamEvent ee: examEvents){
			if(ee.getClassroom() != null)
			salleExam.add(ee);	
		}
		return salleExam;
	}
}